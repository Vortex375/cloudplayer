<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
             xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
             ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
             ui:generateKeys="com.google.gwt.i18n.server.keygen.MD5KeyGenerator"
             ui:generateFilename="Messages"
        >
    <!-- Menu Area is 2 columns wide, so we can use 2 columns in this view -->
    <g:HTMLPanel>
        <b:NavList>
            <b:NavLink ui:field="homeButton" icon="HOME" active="true"><ui:msg>Home</ui:msg></b:NavLink>
            <b:NavHeader>
                <ui:msg>Music</ui:msg>
            </b:NavHeader>
            <b:NavLink ui:field="searchButton" icon="SEARCH">
                <ui:msg>Search</ui:msg>
            </b:NavLink>
            <b:NavLink ui:field="browseButton" icon="BOOK">
                <ui:msg>Browse</ui:msg>
            </b:NavLink>
            <b:NavLink ui:field="foldersButton" icon="FOLDER_CLOSE">
                <ui:msg>Folders</ui:msg>
            </b:NavLink>
            <b:NavLink ui:field="playlistsButton" icon="LIST_ALT">
                <ui:msg>Playlists</ui:msg>
            </b:NavLink>
            <b:NavHeader>
                <ui:msg>Heart List</ui:msg>
            </b:NavHeader>
            <b:NavHeader>
                <ui:msg>Settings</ui:msg>
            </b:NavHeader>
            <b:NavLink ui:field="profileButton" icon="USER">
                <ui:msg>Profile</ui:msg>
            </b:NavLink>
            <b:NavLink ui:field="settingsButton" icon="COG">
                <ui:msg>Settings</ui:msg>
            </b:NavLink>
            <b:NavLink ui:field="adminButton" icon="COGS">
                <ui:msg>Admin</ui:msg>
            </b:NavLink>
        </b:NavList>
    </g:HTMLPanel>
</ui:UiBinder>